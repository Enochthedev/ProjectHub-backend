{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "wave",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:projects": "jest --config ./test/jest-e2e.json --testPathPatterns=projects-api.e2e-spec.ts",
    "test:e2e:bookmarks": "jest --config ./test/jest-e2e.json --testPathPatterns=bookmarks-api.e2e-spec.ts",
    "test:e2e:admin": "jest --config ./test/jest-e2e.json --testPathPatterns=admin-api.e2e-spec.ts",
    "test:performance": "jest --config ./test/jest-e2e.json --testPathPatterns=performance-basic.e2e-spec.ts --testTimeout=30000",
    "test:integration": "jest --config ./test/jest-e2e.json --testPathPatterns='(projects-api|bookmarks-api|admin-api).e2e-spec.ts'",
    "test:all": "npm run test && npm run test:e2e",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/typeorm.config.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "seed:milestones": "ts-node -r tsconfig-paths/register src/seeds/milestone-seeder.ts",
    "milestone:dev-cli": "ts-node -r tsconfig-paths/register src/utils/milestone-dev-cli.ts",
    "milestone:visualize": "npm run milestone:dev-cli visualize",
    "milestone:test": "npm run milestone:dev-cli test",
    "milestone:validate": "npm run milestone:dev-cli validate",
    "milestone:cleanup": "npm run milestone:dev-cli cleanup",
    "seed": "ts-node -r tsconfig-paths/register src/seeds/seed.ts seed",
    "seed:rollback": "ts-node -r tsconfig-paths/register src/seeds/seed.ts rollback",
    "seed:projects": "ts-node -r tsconfig-paths/register src/seeds/seed.ts projects",
    "seed:cleanup": "ts-node -r tsconfig-paths/register src/seeds/seed.ts cleanup",
    "test:data": "ts-node -r tsconfig-paths/register src/test/performance/data-generator.ts",
    "test:data:small": "npm run test:data small",
    "test:data:medium": "npm run test:data medium",
    "test:data:large": "npm run test:data large",
    "test:data:cleanup": "npm run test:data cleanup",
    "test:data:stats": "npm run test:data stats",
    "test:perf": "ts-node -r tsconfig-paths/register src/test/performance/performance-test.script.ts",
    "test:perf:minimal": "npm run test:perf minimal",
    "test:perf:realistic": "npm run test:perf realistic",
    "test:perf:stress": "npm run test:perf stress",
    "test:perf:benchmark": "npm run test:perf benchmark",
    "test:performance:stress": "npm run test:performance stress",
    "test:performance:benchmark": "npm run test:performance benchmark",
    "seed:recommendations": "ts-node -r tsconfig-paths/register src/test/utils/seed-recommendations.command.ts",
    "seed:recommendations:scenarios": "npm run seed:recommendations -- --scenarios --diverse",
    "seed:recommendations:performance": "npm run seed:recommendations -- --performance --size medium",
    "seed:recommendations:all": "npm run seed:recommendations -- --scenarios --diverse --performance --size small",
    "seed:recommendations:clean": "npm run seed:recommendations -- --clean",
    "benchmark:recommendations": "ts-node -r tsconfig-paths/register src/test/performance/benchmark-runner.ts",
    "benchmark:recommendations:accuracy": "npm run benchmark:recommendations -- --accuracy-only",
    "benchmark:recommendations:performance": "npm run benchmark:recommendations -- --performance-only",
    "benchmark:recommendations:load": "npm run benchmark:recommendations -- --load-test",
    "benchmark:recommendations:ab": "npm run benchmark:recommendations -- --ab-test",
    "benchmark:recommendations:full": "npm run benchmark:recommendations -- --load-test --ab-test"
  },
  "dependencies": {
    "@huggingface/inference": "^4.8.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/helmet": "^0.0.48",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "helmet": "^8.1.0",
    "ical-generator": "^9.0.0",
    "joi": "^18.0.1",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/joi": "^17.2.2",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
